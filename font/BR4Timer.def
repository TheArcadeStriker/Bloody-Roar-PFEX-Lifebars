[FNT v2]
; FNT v2 version number.  Don't change this.
fntversion = 2,00
; Name of this font.
name = "BR4Timer"
; This is a template def file for fonts by TheArcadeStriker.
; Just replace the name and sff file name accordingly to what
; you want to name this font.

[Def]
; This is a bitmap font, keep it like this
Type = bitmap
; Size of font: width, height.  Width is used for spaces.
; Use the maximal size of the font characters here (width, height)
Size = 16,38
; Spacing between font glyphs: width, height.
; You can use negative numbers to decreate the default spacing.
Spacing = 1,0
; Drawing offset: x, y.
Offset = 0,0
; Filename of the sff containing the glyphs.  Use sff v2 only.
File = BR4Timer.sff

; Note: All units are in pixels.
; Text rendered with bitmap fonts may be in ASCII only.
; See files in work/font for information on creating the sff.